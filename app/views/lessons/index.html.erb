<div class="container">
  <div class="row">
    <div class="col-lg-12">
      Lessons Main
    </div>
  </div>
  <% @lessons.each do |lesson| %>
  <%= lesson.name %>
  <%= lesson.description %>
  <%= lesson.syntax %>

  <%= link_to 'Edit Lesson', edit_lesson_path(lesson) %>
  <%= link_to 'Delete Lesson', lesson_path(lesson), method: :delete %>

  <% end %>
</div>
